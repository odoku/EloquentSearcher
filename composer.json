{
    "name": "odoku/eloquent-searcher",
    "description": "Give searching power to Eloquent.",
    "license": "MIT",
    "authors": [
        {
            "name": "odoku",
            "email": "m.onogawa@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/database": "~5.3.0|~5.4.0",
        "illuminate/support": "~5.3.0|~5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^5.0",
        "orchestra/testbench": "~3.3.0|~3.4.0"
    },
    "autoload": {
        "psr-4": {
          "EloquentSearcher\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EloquentSearcher\\Test\\": "tests"
        }
    },
    "minimum-stability": "stable"
}
